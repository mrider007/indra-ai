{
  "version": 2,
  "name": "indra-ai-api",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key",
    "REDIS_URL": "@redis_url",
    "ENABLE_PRO_FEATURES": "true"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  }
}
